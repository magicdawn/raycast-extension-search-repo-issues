{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-specific-issue",
  "title": "search-specific-issue",
  "description": "search-specific-issue",
  "icon": "command-icon.png",
  "author": "magicdawn",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Issue in specific repo",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view",
      "preferences": [
        {
          "name": "repo",
          "title": "repository",
          "description": "repository in <username>/<repname> format",
          "placeholder": "<username>/<repname>",
          "type": "textfield",
          "required": true
        },
        {
          "name": "ghToken",
          "title": "GitHub API token",
          "description": "GitHub API token",
          "type": "textfield",
          "required": true
        },
        {
          "name": "httpProxy",
          "title": "using proxy",
          "description": "using httpProxy for github api request",
          "placeholder": "e.g http://127.0.0.1:7890",
          "type": "textfield",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.5",
    "ahooks": "^3.7.5",
    "conf": "^10.2.0",
    "fs-extra": "^11.1.0",
    "ms": "^2.1.3",
    "octokit": "^2.0.14",
    "proxy-agent-tweak": "^5.0.1",
    "react": "^18.2.0",
    "reuse-promise": "^2.0.0",
    "valtio": "^1.10.3"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "18",
    "@types/react": "18",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "prepare": "husky install"
  },
  "pnpm": {
    "patchedDependencies": {}
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  }
}
