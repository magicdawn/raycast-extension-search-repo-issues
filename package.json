{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-specific-issue",
  "type": "module",
  "title": "search-specific-issue",
  "description": "search-specific-issue",
  "icon": "command-icon.png",
  "author": "magicdawn",
  "categories": [
    "Developer Tools"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "ray develop",
    "typecheck": "tsc --noEmit",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "build": "ray build -e dist",
    "publish": "ray publish"
  },
  "commands": [
    {
      "name": "index",
      "title": "Search Issue in specific repo",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view",
      "preferences": [
        {
          "name": "repo",
          "title": "repository",
          "description": "repository in <username>/<repname> format",
          "placeholder": "<username>/<repname>",
          "type": "textfield",
          "required": true
        },
        {
          "name": "ghToken",
          "title": "GitHub API token",
          "description": "GitHub API token",
          "type": "textfield",
          "required": true
        }
      ]
    }
  ],
  "pnpm": {
    "patchedDependencies": {}
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@raycast/api": "^1.88.4",
    "ahooks": "^3.8.4",
    "conf": "^13.1.0",
    "fs-extra": "^11.2.0",
    "fzf": "^0.5.2",
    "lodash-es": "^4.17.21",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "octokit": "^4.0.3",
    "p-memoize": "^7.1.1",
    "react": "^19.0.0",
    "valtio": "^2.1.2"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/ms": "^0.7.34",
    "@types/node": "22",
    "@types/react": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321"
}
