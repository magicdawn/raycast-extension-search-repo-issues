{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-specific-issue",
  "title": "search-specific-issue",
  "description": "search-specific-issue",
  "icon": "command-icon.png",
  "author": "magicdawn",
  "categories": [
    "Developer Tools"
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "prepare": "husky install"
  },
  "commands": [
    {
      "name": "index",
      "title": "Search Issue in specific repo",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view",
      "preferences": [
        {
          "name": "repo",
          "title": "repository",
          "description": "repository in <username>/<repname> format",
          "placeholder": "<username>/<repname>",
          "type": "textfield",
          "required": true
        },
        {
          "name": "ghToken",
          "title": "GitHub API token",
          "description": "GitHub API token",
          "type": "textfield",
          "required": true
        }
      ]
    }
  ],
  "pnpm": {
    "patchedDependencies": {}
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "@raycast/api": "^1.60.1",
    "ahooks": "^3.7.8",
    "conf": "^10.2.0",
    "fs-extra": "^11.1.1",
    "ms": "^2.1.3",
    "octokit": "^2.1.0",
    "proxy-agent-tweak": "^5.0.1",
    "react": "^18.2.0",
    "reuse-promise": "^2.0.0",
    "valtio": "^1.11.3"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@types/fs-extra": "^11.0.3",
    "@types/ms": "^0.7.33",
    "@types/node": "20",
    "@types/react": "18",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
